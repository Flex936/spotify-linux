{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "spotify-linux",
  "version": "0.1.0",
  "identifier": "com.tancsikalex.spotify-linux",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "https://open.spotify.com",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "spotify-linux",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}